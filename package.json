{
  "name": "site",
  "version": "1.0.0",
  "type": "module",
  "main": ".eleventy.js",
  "scripts": {
    "start": "npx eleventy --serve",
    "clean": "rm -rf _site package-lock.json node_modules && npm cache clean --force",
    "erase": "rm -rf _site .cache curious-grapes.one",
    "build": "npx tailwindcss -i ./tailwind.css -o ./src/assets/css/tailwind.css --postcss --watch",
    "prod": "npx tailwindcss -i ./tailwind.css -o ./src/assets/css/tailwind.css --postcss --minify && npx eleventy --output=curious-grapes.one",
    "dev": "npm-run-all --parallel build start"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@11ty/eleventy-img": "^6.0.1",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
    "@quasibit/eleventy-plugin-sitemap": "^2.2.0",
    "@tailwindcss/typography": "^0.5.16",
    "luxon": "^3.0.0",
    "markdown-it": "^14.1.0",
    "markdown-it-anchor": "^9.2.0",
    "postcss-cli": "^11.0.0"
  },
  "devDependencies": {
    "@11ty/eleventy": "^3.0.0",
    "autoprefixer": "^10.4.20",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.1",
    "tailwindcss": "^3.0.18"
  }
}
